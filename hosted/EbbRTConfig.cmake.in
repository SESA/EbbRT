@PACKAGE_INIT@
# - Config file for the EbbRT hosted package
# It defines the following variables
#  EBBRT_INCLUDE_DIRS - include directories for EbbRT
#  EBBRT_LIBRARIES    - libraries to link against

set_and_check(EBBRT_INCLUDE_DIRS "@PACKAGE_INSTALL_INCLUDE_DIR@")
set_and_check(EBBRT_LIBRARIES "@PACKAGE_INSTALL_LIB_DIR@libEbbRT.a")

check_required_components(EbbRT)
